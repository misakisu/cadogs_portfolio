<h4>管理者画面</h4>
<table class="table table-inverse">
    <tr><th>氏名</th><td><%= @owner.name %></td></tr>
    <tr><th>電話番号</th><td><%= @owner.phone_number %></td></tr>
    <tr><th>メールアドレス</th><td><%= @owner.email %></td></tr>
    <tr><th><button><%= link_to "編集", edit_owner_registration_path %></button></td></tr>
</table>
<h4>登録済みホテル（一覧)</h4>
  <% @hotels.each do |hotel| %>
  <%= render partial: 'hotels/hotels', locals: {hotel: hotel} %>
  <div><%= link_to '登録情報編集', edit_owner_hotel_path(hotel), class: "btn btn-success" %></div>
  <% end %>